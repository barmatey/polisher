{
  "manifest_version": 3,
  "name": "Vue Extension",
  "version": "0.0.1",
  "description": "Chrome Extension на Vue 3 + Vite",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/main.js"]
    }
  ],
  "permissions": []
}
